<!DOCTYPE html>
<html lang="en">

<head>
  <!-- METADATA -->
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta property="og:image" content="PICTURE-PREVIEW">
  <meta property="og:image:type" content="image/png">
  <meta property="og:image:width" content="1200">
  <meta property="og:image:height" content="630">
  <!-- METADATA -->

  <title>Sympho-niche</title>

  <link rel=icon href="FAVICON-DARK" media="(prefers-color-scheme: light)" type="image/png">

  <link rel=icon href="FAVICON-LIGHT" media="(prefers-color-scheme: dark)" type="image/png">

  <!-- GOOGLE FONTS -->
  <link rel="preconnect" href="https://fonts.googleapis.com">
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
  <!-- GOOGLE FONTS -->

  <!-- FONT AWESOME -->
  <script src="https://kit.fontawesome.com/ad98de5722.js" crossorigin="anonymous"></script>
  <!-- FONT AWESOME -->

  <!-- CUSTOM STYLESHEETS -->
  <link rel="stylesheet" href="stylesheets/style.css">
  <!-- CUSTOM STYLESHEETS -->
</head>

<body>
  <!-- HTML code here -->
  <%- include("partials/navbar", {page: 2}) %>

    <section id="view">
      <div class="container">
        <p class="section-title2">
          View All Data On The Database
        </p>
        <table>
          <thead>
            <tr>
              <th>Song</th>
              <th>Artist</th>
              <th>Album</th>
            </tr>
          </thead>
          <tbody>
            <% rows.forEach((row)=> { %>
              <tr>
                <td>
                  <%= row.Song %>
                </td>
                <td>
                  <%= row.Artist %>
                </td>
                <td>
                  <%= row.Album %>
                </td>
              </tr>
              <% }) %>
          </tbody>
        </table>
      </div>

    </section>


    <%- include("partials/footer.ejs") %>

      <!-- SCRIPTS -->
      <script src="https://kit.fontawesome.com/ad98de5722.js" crossorigin="anonymous"></script>
      <script type="text/javascript" src="/javascripts/script.js"></script>
      <!-- SCRIPTS -->
</body>
